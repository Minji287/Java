package chapter3;

public class Ex3_2 {

	public static void main(String[] args) {
		int i = 5, j = 0; // 1. 변수 값 저장
		
		j = i++; // 2. j에 i를 먼저 넣어주고(j = i) -> i + 1
		System.out.println("j=i++; 실행 후, i=" + i + ", j=" + j); // 3. 실행 -> i=6, j=5

		
//		i = 5;
//		j = 0;
//		
//		j = ++i;
//		System.out.println("j=++i; 실행 후, i=" + i + ", j=" + j);
	}

}
