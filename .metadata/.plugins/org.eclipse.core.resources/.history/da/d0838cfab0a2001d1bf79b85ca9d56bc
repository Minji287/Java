package Sample_2;

public class Test_6 {

	public static void main(String[] args) {
		System.out.println(10/3);
		System.out.println(10.0/3);
		System.out.printf("%.2f%n", 10.0/3); // f는 실수로 표현
		System.out.printf("%d", 0x1A); // d는 10진수
		System.out.println();
		System.out.printf("%x", 0x1A); // x는 16진수
	}

}
