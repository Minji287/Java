package chapter3;

public class Ex3_11 {

	public static void main(String[] args) {
		double pi = 3.141592;
		double shortPi = Math.round(pi * 1000) / 1000.0;
							// 1. = (3141.592)
							// 2. 		3142 / 1000.0 = 3.142
		System.out.println(shortPi);
		
		double shortPi2 = Math.round(pi / 1000.0);
		System.out.println(shortPi2);
		
	}

}
