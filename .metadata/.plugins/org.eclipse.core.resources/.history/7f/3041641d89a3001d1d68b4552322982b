package chapter3;

public class Ex3_13 {

	public static void main(String[] args) {
		int a = 20;
		int b = 30;
		
		if(a > b) {
			System.out.println("예");
			System.out.println("a = " + a + "이고 b = " + b + "입니다");
		}
		else {
			System.out.println("아니요");
			System.out.println("a = " + a + "이고 b = " + b + "입니다");
		}
		
//		if(a > b) {
//			System.out.println("아니요");
//			System.out.println("b가 더 커요");
//		}
	}

}
