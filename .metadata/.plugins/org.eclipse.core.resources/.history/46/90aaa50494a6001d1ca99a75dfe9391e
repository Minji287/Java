package package_1;

public class Test_5 {

	public static void main(String[] args) {
		int ss = (int)3.14; // 강제 형변환
		System.out.println(ss);
		double kk = 3; // 자동 형변환
		System.out.println(kk);
		
		int j = 0;
		int i = 0;
		
		j = i++;
		System.out.println("j= " + j);
		System.out.println("i= " + i);
		
		String str = "yes";
		System.out.println(str.equals("YES"));
		System.out.println("yes");
	}

}
